{
  "@context": {
    "whg": "https://whgazetteer.org/schema/whg#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "geo": "http://www.opengis.net/ont/geosparql#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  },
  "@graph": [
    {
      "@id": "whg:Place",
      "@type": "rdfs:Class",
      "rdfs:label": "Place",
      "rdfs:comment": "A named geographic entity with spatial, temporal, and administrative properties."
    },
    {
      "@id": "whg:Dataset",
      "@type": "rdfs:Class",
      "rdfs:label": "Dataset",
      "rdfs:comment": "A dataset contributed to WHG, containing places and metadata."
    },
    {
      "@id": "whg:Collection",
      "@type": "rdfs:Class",
      "rdfs:label": "Collection",
      "rdfs:comment": "A collection of Datasets and/or Places grouped thematically or by project."
    },
    {
      "@id": "whg:Area",
      "@type": "rdfs:Class",
      "rdfs:label": "Area",
      "rdfs:comment": "A defined geographic area of interest, often used for filtering or grouping places."
    },

    {
      "@id": "whg:identifier",
      "@type": "rdf:Property",
      "rdfs:label": "Persistent Identifier",
      "rdfs:domain": "rdfs:Resource",
      "rdfs:range": "xsd:anyURI",
      "rdfs:comment": "Persistent URI identifying a WHG entity.",
      "whg:example": "https://whgazetteer.org/place/12345"
    },
    {
      "@id": "whg:name",
      "@type": "rdf:Property",
      "rdfs:label": "Name",
      "rdfs:domain": "rdfs:Resource",
      "rdfs:range": "xsd:string",
      "rdfs:comment": "Canonical name of the entity.",
      "whg:example": "London"
    },
    {
      "@id": "whg:alternateName",
      "@type": "rdf:Property",
      "rdfs:label": "Alternate Name",
      "rdfs:domain": "whg:Place",
      "rdfs:range": "xsd:string",
      "rdfs:comment": "Alternative names or historical variants.",
      "whg:example": "Londinium"
    },
    {
      "@id": "whg:fclass",
      "@type": "rdf:Property",
      "rdfs:label": "Feature Class",
      "rdfs:domain": "whg:Place",
      "rdfs:range": "xsd:string",
      "rdfs:comment": "The WHG feature class of the place. Must be one of the GeoNames feature classes.",
      "whg:allowedValues": ["A","H","L","P","R","S","T","U","V","X"],
      "whg:reference": "https://www.geonames.org/source-code/javadoc/org/geonames/FeatureClass.html"
    },
    {
      "@id": "whg:geometry",
      "@type": "rdf:Property",
      "rdfs:label": "Geometry",
      "rdfs:domain": "whg:Place",
      "rdfs:range": "geo:Geometry",
      "rdfs:comment": "Spatial representation of the place.",
      "whg:example": {"type":"Point","coordinates":[-0.1278,51.5074]}
    },
    {
      "@id": "whg:timespans",
      "@type": "rdf:Property",
      "rdfs:label": "Timespans",
      "rdfs:domain": "whg:Place",
      "rdfs:range": "xsd:string",
      "rdfs:comment": "Temporal coverage or existence of the place (ISO dates or year ranges).",
      "whg:example": "1200-1600"
    },
    {
      "@id": "whg:ccodes",
      "@type": "rdf:Property",
      "rdfs:label": "Country Codes",
      "rdfs:domain": "whg:Place",
      "rdfs:range": "xsd:string",
      "rdfs:comment": "ISO 2-letter country codes where the place is located.",
      "whg:reference": "https://en.wikipedia.org/wiki/ISO_3166-1_alpha-2",
      "whg:example": "GB"
    },

    {
      "@id": "whg:dataset",
      "@type": "rdf:Property",
      "rdfs:label": "Dataset",
      "rdfs:domain": "whg:Place",
      "rdfs:range": "whg:Dataset",
      "rdfs:comment": "Dataset in which this place appears."
    },
    {
      "@id": "whg:collection",
      "@type": "rdf:Property",
      "rdfs:label": "Collection",
      "rdfs:domain": "whg:Dataset",
      "rdfs:range": "whg:Collection",
      "rdfs:comment": "Collection to which this dataset belongs."
    },
    {
      "@id": "whg:hasMember",
      "@type": "rdf:Property",
      "rdfs:label": "Has Member",
      "rdfs:domain": "whg:Collection",
      "rdfs:range": "rdfs:Resource",
      "rdfs:comment": "Member entity of this collection (can be a Dataset or a Place)."
    },
    {
      "@id": "whg:withinArea",
      "@type": "rdf:Property",
      "rdfs:label": "Within Area",
      "rdfs:domain": "whg:Place",
      "rdfs:range": "whg:Area",
      "rdfs:comment": "The geographic area within which this place is located."
    }
  ]
}
